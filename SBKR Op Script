    local function External()
        notif = function(title,text,duration)
            game.StarterGui:SetCore("SendNotification", {
                Title = title,
                Text = text,
                Duration = duration
            })
        end
        print'This is made by jigglebasketballz, tracks players kills and gloves.'
        print'If you are gonna showcase CREDIT!!! or show console lol'
        wait(1)
        for i,plr in pairs(game.Players:GetChildren()) do
            local Pleyer = plr
            local Leaderstats = Pleyer.leaderstats
            local Kills = Leaderstats.Kills
            local Glove = Leaderstats.Glove
            Kills:GetPropertyChangedSignal("Value"):Connect(function()
                notif('Kills', Pleyer.Name..' now has: ' ..tostring(Kills.Value)..' kills.', 1)
            end)
            Glove:GetPropertyChangedSignal("Value"):Connect(function()
                notif('Glove', Pleyer.Name..' equipped the glove: ' ..tostring(Glove.Value)..'.', 1)
            end)
        end
    end

    External()

    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
