    local lengthy = 'u'
    function lenghty_Fifty_Kills()
    for i=1,30 do
    _50_Kills:FireServer()
    end
    end

    Mouse.KeyDown:Connect(function(k)
    if k==lengthy then
    lenghty_Fifty_Kills()
    end
    end)

    Mouse.KeyDown:Connect(function(k)
        if k == OtherKey then
            Other:FireServer()
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == EventKey then
            Event:FireServer()
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == RandomKey then
            RandomEvent:FireServer()
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == killsKey then
            kills:FireServer()
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == deadkey then
            game.Players.LocalPlayer.Character.Humanoid.Health = 0
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == rejoinkey then
           local pi = 11601030184
           local plr = game.Players.LocalPlayer
           local tps = game:GetService("TeleportService")
           tps:Teleport(pi, plr) 
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == anchorkey then
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == unanchorkey then
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == BIG_KILLS_OMG then
            for i=1,1000 do
                kills:FireServer()
            end
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == Fivekills then
            for i=1,500 do
                kills:FireServer()
            end
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == crocs then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == Legit_Crocs then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 28
        end
    end)
    Mouse.KeyDown:Connect(function(k)
        if k == dw then
            doesnt_work:FireServer()
