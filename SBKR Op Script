    local _50_Kills = game:GetService("ReplicatedStorage").TimelessEvents.test2
    local Player = game.Players.LocalPlayer
    local Mouse = Player:GetMouse()
    local OtherKey = 'm'
    local RandomKey = 'c'
    local EventKey = 'r'
    local killsKey = 'n'
    local deadkey = 'p'
    local rejoinkey = 'h'
    local anchorkey = 'j'
    local unanchorkey = 'k'
