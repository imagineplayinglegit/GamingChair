function whyisitsolongomgimgonnacry()
    local wordstable = {
          ["OtherKey"] = 'OtherKey: m',
          ["RandomKey"] = 'RandomKey: c',
          ["EventKey"] = 'EventKey: r',
          ["killsKey"] = 'killsKey: n',
          ["deadkey"] = 'deadkey: p',
          ["rejoinkey"] = 'rejoinkey: h',
          ["anchorkey"] = 'anchorkey: j',
          ["unanchorkey"] = 'unanchorkey: k',
          ["BIG_KILLS_OMG"] = 'BIG_KILLS_OMG: u',
          ["Fivekills"] = 'Fivekills: ',
          ["Legit_Crocs"] = 'Legit_Crocs: v',
          ["dw"] = 'dw: z',
          ["Fifty_kills"] = 'Fifty_kills: f',
          ["lenghty"] = 'lenghty: u'
    }
    for i,v in pairs(wordstable) do
        warn(v)
    end
end


